# lib_dir needs to be visible to dlopen().
# /usr/lib and friends are the best bet, but any path can be used if it is
# added to LD_LIBRARY_PATH.
if [ -d "/usr/lib64" ]; then
    lib_dir="/usr/lib64"
else
    lib_dir="/usr/lib"
fi

bin_dir="/usr/bin"

inc_dir="/usr/include"

plug_dir="${HOME}/.yed"
